[{"id":"202","title":"\u003Ca href=\u0022\/tourist-spots\/malongcay-hotspring\u0022 hreflang=\u0022en\u0022\u003EMalongcay Hotspring\u003C\/a\u003E","location":"\u003Ca href=\u0022\/taxonomy\/term\/15\u0022 hreflang=\u0022en\u0022\u003EMalongcay\u003C\/a\u003E","image":"","map":"","category":"Tourist Spots","details":"\u003Cp\u003EContains Sulfur content with water source from small lagoon\u003C\/p\u003E"},{"id":"199","title":"\u003Ca href=\u0022\/tourist-spots\/mag-aso-steam-cave\u0022 hreflang=\u0022en\u0022\u003EMag-aso Steam Cave\u003C\/a\u003E","location":"\u003Ca href=\u0022\/taxonomy\/term\/16\u0022 hreflang=\u0022en\u0022\u003EMag-aso\u003C\/a\u003E","image":"","map":"","category":"Tourist Spots","details":"\u003Cp\u003EMag-aso Steam Cave aslo known as \u0022Oko Cave\u0022\u003C\/p\u003E"},{"id":"196","title":"\u003Ca href=\u0022\/tourist-spots\/mainit-falls\u0022 hreflang=\u0022en\u0022\u003EMainit Falls\u003C\/a\u003E","location":"\u003Ca href=\u0022\/taxonomy\/term\/18\u0022 hreflang=\u0022en\u0022\u003EBidjao\u003C\/a\u003E","image":"","map":"","category":"Tourist Spots","details":"\u003Cp\u003ECTTO: JM Villanueva\u003C\/p\u003E"},{"id":"195","title":"\u003Ca href=\u0022\/tourist-spots\/mainit-river\u0022 hreflang=\u0022en\u0022\u003EMainit River\u003C\/a\u003E","location":"\u003Ca href=\u0022\/taxonomy\/term\/10\u0022 hreflang=\u0022en\u0022\u003EBulak\u003C\/a\u003E","image":"","map":"","category":"Tourist Spots","details":"\u003Cp\u003EMainit River\u00a0\u003C\/p\u003E"},{"id":"194","title":"\u003Ca href=\u0022\/tourist-spots\/nasig-id-water-reservoir\u0022 hreflang=\u0022en\u0022\u003ENasig-id Water Reservoir\u003C\/a\u003E","location":"\u003Ca href=\u0022\/taxonomy\/term\/12\u0022 hreflang=\u0022en\u0022\u003ECasile\u003C\/a\u003E","image":"","map":"","category":"Tourist Spots","details":"\u003Cp\u003ENasidig\u00a0\u003C\/p\u003E"},{"id":"193","title":"\u003Ca href=\u0022\/tourist-spots\/mahogany-resort\u0022 hreflang=\u0022en\u0022\u003EMahogany Resort\u003C\/a\u003E","location":"\u003Ca href=\u0022\/taxonomy\/term\/14\u0022 hreflang=\u0022en\u0022\u003EPoblacion District 2\u003C\/a\u003E","image":"","map":"","category":"Tourist Spots","details":"\u003Cp\u003EOur place is a small family run resort located on 3 hectares of quiet landscape of nature. A moment away from Dauin Dive Resort Area. Newly built clean and spacious room with double bed, private shared bathrooms. We also have restaurant and bar in the area that offers delicious food and refreshing drinks at very affordable prices.\u003C\/p\u003E"},{"id":"188","title":"\u003Ca href=\u0022\/tourist-spots\/bidjao-overlooking-view\u0022 hreflang=\u0022en\u0022\u003EBidjao Overlooking View\u003C\/a\u003E","location":"\u003Ca href=\u0022\/taxonomy\/term\/18\u0022 hreflang=\u0022en\u0022\u003EBidjao\u003C\/a\u003E","image":"","map":"","category":"Tourist Spots","details":"\u003Cp\u003EBidjao ,Old Palimpinun Geothermal Site\u003C\/p\u003E"},{"id":"187","title":"\u003Ca href=\u0022\/tourist-spots\/silver-reef-resort\u0022 hreflang=\u0022en\u0022\u003ESilver Reef Resort\u003C\/a\u003E","location":"\u003Ca href=\u0022\/taxonomy\/term\/11\u0022 hreflang=\u0022en\u0022\u003EMaayong Tubig\u003C\/a\u003E","image":"","map":"","category":"Tourist Spots","details":"\u003Cp\u003EAs a full circle, up-cycling, renewable and energy efficient, solid waste to water management committed practices resort, we simply don\u0027t contribute to landfill and ocean pollution. Divers are given a free eco education on zero impact diving and preservation of marine life. Furthermore, direct actions from natural coral rescue and reef rehabilitation through trash and ghost net extraction are done as needed on every dive.\u003C\/p\u003E"},{"id":"184","title":"\u003Ca href=\u0022\/schools\/apo-es\u0022 hreflang=\u0022en\u0022\u003EAPO E\/S\u003C\/a\u003E","location":"\u003Ca href=\u0022\/taxonomy\/term\/4\u0022 hreflang=\u0022en\u0022\u003EApo Island\u003C\/a\u003E","image":"","map":"","category":"Schools","details":"\u003Cp\u003EIt was truly a\u00a0privilege to see the Apo Island Marine Reserve and Fish Sanctuary. There is a partnership between the community and the Silliman University in Dumaguete to keep this area protected and because of their success we can explore the underwater beauty. Apo is a true jewel on this planet of ours.\u003C\/p\u003E"},{"id":"183","title":"\u003Ca href=\u0022\/tourist-spots\/apo-island\u0022 hreflang=\u0022en\u0022\u003EApo Island\u003C\/a\u003E","location":"\u003Ca href=\u0022\/taxonomy\/term\/4\u0022 hreflang=\u0022en\u0022\u003EApo Island\u003C\/a\u003E","image":"","map":"","category":"Tourist Spots","details":"\u003Cp\u003E\u003Cstrong\u003EApo Island\u003C\/strong\u003E\u00a0is a volcanic island covering 12\u00a0\u003Ca href=\u0022http:\/\/en.wikipedia.org\/wiki\/Hectare\u0022\u003Ehectares\u003C\/a\u003E\u00a0in land area, 7 kilometers off the southeastern tip of\u00a0\u003Ca href=\u0022http:\/\/en.wikipedia.org\/wiki\/Negros_Island\u0022\u003ENegros Island\u003C\/a\u003E\u00a0and 30 kilometers south of the\u00a0\u003Ca href=\u0022http:\/\/en.wikipedia.org\/wiki\/Negros_Oriental\u0022\u003ENegros Oriental\u003C\/a\u003E\u00a0capital of\u00a0\u003Ca href=\u0022http:\/\/en.wikipedia.org\/wiki\/Dumaguete_City\u0022\u003EDumaguete City\u003C\/a\u003E\u00a0in the\u00a0\u003Ca href=\u0022http:\/\/en.wikipedia.org\/wiki\/Philippines\u0022\u003EPhilippines\u003C\/a\u003E. The name \u201cApo\u201d was derived from the a Filipino word which literally means, \u201cGrandchild\u201d.\u003C\/p\u003E"}]