
{% if id == "homapage_bootstrap" %}
  {% if rows %}
    <section class="about">
      <div class="row">
        <div class="col-md-12">
          <div class="tourist-spot-title" data-aos="fade-up" data-aos-anchor-placement="bottom-bottom" data-aos-duration="1000">
            <h4>TOURIST SPOTS</h4>
          </div>
        </div>
      </div>
      <div class="row">
        {{ rows }}
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 col"><a href="/tourist_spots">
          <div class="item-new">
            <a class="btn btn-primary"><span>More</span></a>
          </div></a>
        </div>
      </div>
    </section>  
  {% endif %}   
{# {% endif %}  #}


{% elseif id == "tourist_spots" %}
  {% if rows %}
      <section id="services">
        <div class="container">
          {{ rows }}
        </div>
      </section>
  {% endif %}
{# {% endif %}       #}
  

{% elseif id == "schools" %}
  {% if rows %}
    <section id="blog">
      <div class="container-fluid">
        <div class="row">
            <h4>Explore</h4>
            <h6>Explore the Beauty and Location of Our Schools in Dauin</h6>
            {{ rows }}
            <div class="col-xs-12 col-sm-6 col-md-3 item-new">
              <a class="btn btn-primary" href="/schools"><span>More</span></a>
            </div>
        </div>
      </div>
    </section> 
  {% endif %}
{# {% endif %} #}




{% elseif id == "events" %}
  {% if rows %}
    {{ rows }}
  {% endif %}  
{# {% endif %}  #}


{% elseif id == "students" %}
  {% if rows %}
    <section class="customers">   
      <div class="container">
        <div class="row">
          <div class="col-md-8 col-md-offset-2">
            <div class="carousel slide" id="fade-quote-carousel">
              <div class="carousel-inner" role="listbox">
                  {{ rows }}
               </div>    
                   <a class="left carousel-control" href="#fade-quote-carousel" role="button" data-slide="prev">
                    <i class="fa fa-angle-left" aria-hidden="true"></i>
                    <span class="sr-only">Previous</span>
                  </a>
                  <a class="right carousel-control" href="#fade-quote-carousel" role="button" data-slide="next">
                    <i class="fa fa-angle-right" aria-hidden="true"></i>
                    <span class="sr-only">Next</span>
                  </a>
            </div>              
          </div>
        </div>  
      </div>
    </section>
  {% endif %}
{% else %}
  {% if rows %}
    {{ rows }}
  {% endif %}
{% endif %}



{% if pager %}
  <section id="footer-blog">
    <div class="container-fluid">
      <div class="row">
        {{ pager }}
      </div>
    </div>
  </section>
{% endif %}  