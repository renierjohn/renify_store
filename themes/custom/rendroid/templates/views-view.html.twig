
{% if id == "homapage_bootstrap" %}
  {% if rows %}
    <section class="about">
      <div class="row">
        <div class="col-md-12">
          <div class="tourist-spot-title" data-aos="fade-up" data-aos-anchor-placement="bottom-bottom" data-aos-duration="1000">
            <h4>TOURIST SPOTS</h4>
          </div>
        </div>
      </div>
      <div class="row">
        {{ rows }}
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 col">
          <div class="item-new">
            <a class="btn btn-primary" href="/tourist_spots"><span>More</span></a>
          </div>
        </div>
      </div>
    </section>
  {% endif %}
{# {% endif %}  #}


{% elseif id == "tourist_spots" %}
  {% if rows %}
      <section id="services">
        <div class="container">
           <div class="row">
             <div class="social-media">
                <div class="row">
                      <div class="col-md-12 col-sm-12 col-xs-12 fb-profile-picture" ></div>
                </div>
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12 fb-fullname" ></div>
                </div>
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12 fb-like" data-href="https://dev-deped-dauin.pantheonsite.io/tourist_spots" data-width=""  data-layout="button_count" data-action="like" data-size="small" data-share="true">
                    </div>
                </div>
                <div class="row">
                   <div class="col-md-12 col-sm-12 col-xs-12 twitter">
                      <a href="https://twitter.com/intent/tweet?button_hashtag=DauinNiBai&ref_src=twsrc%5Etfw" class="twitter-hashtag-button" data-show-count="false">Tweet #DauinNiBai</a><script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
                    </div>
                </div>
              </div>
           </div>
           <div class="row">
              <div class="col-md-10 col-sm-10 col-xs-12">
                {{ rows }}
              </div>
              <div class="col-md-2 col-md-2 hidden-xs">
                {% if header %}
                  {{ header }}
                {% endif %}
              </div>
          </div>
        </div>
      </section>
  {% endif %}
{# {% endif %}       #}


